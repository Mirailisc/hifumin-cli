{
  "name": "hifumin-cli",
  "version": "1.1.1",
  "description": "a cli tool for hifumin inspired by Mangal",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "types": "./src/index.ts",
  "scripts": {
    "start:dev": "ts-node src/index.ts",
    "start:js": "node dist/index.js",
    "build": "tsc -p .",
    "lint": "eslint src --ext ts --config .eslintrc.cjs",
    "semantic-release": "semantic-release"
  },
  "url": "https://github.com/Mirailisc/hifumin-cli",
  "repository": {
    "url": "https://github.com/Mirailisc/hifumin-cli"
  },
  "author": {
    "name": "Mirailisc",
    "url": "https://github.com/Mirailisc"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "keywords": [
    "hifumin",
    "hifumin.app"
  ],
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "axios": "^1.5.0",
    "cli-progress": "^3.12.0",
    "inquirer": "^8.0.0",
    "nanospinner": "^1.1.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.5",
    "@semantic-release/release-notes-generator": "^11.0.6",
    "@types/cli-progress": "^3.11.0",
    "@types/inquirer": "^8.0.0",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "semantic-release": "^21.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
